  "version": "1.0.0",
  "private": true,
  "description": "A modern ticket management system built with Next.js and Supabase.",
  "main": "index.js",
  "scripts": {
    "backup-env": "bash scripts/backup-env.sh",
    "restore-env": "bash scripts/restore-env.sh",
    "dev": "npm run backup-env && lsof -ti :3000 | xargs kill -9 2>/dev/null || true && lsof -ti :3001 | xargs kill -9 2>/dev/null || true && lsof -ti :3002 | xargs kill -9 2>/dev/null || true && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gmail:generate-secret": "ts-node scripts/generate-cron-secret.ts",
    "gmail:start-polling": "ts-node scripts/gmail-polling.ts",
    "gmail:test": "ts-node scripts/test-gmail-polling.ts",
    "gmail:save-tokens": "ts-node scripts/save-gmail-tokens.ts",
    "create:test-emails": "ts-node scripts/create-test-emails.ts",
    "db:reset": "npm run backup-env && ts-node scripts/db-reset.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prebuild": "tsc --noEmit",
    "check-env": "ts-node scripts/check-env.ts",
    "verify-build": "npm run check-env && npm run lint && tsc --noEmit && next build",
    "predeploy": "npm run verify-build",
    "dev:clean": "lsof -ti :3000 | xargs kill -9 2>/dev/null || true && next dev",
    "test:gmail": "ts-node scripts/test-gmail-flow.ts",
    "check:gmail-watch": "ts-node scripts/check-gmail-watch.ts",
    "setup:org": "ts-node scripts/setup-org.ts",
    "setup:gmail-auth": "ts-node scripts/setup-gmail-auth.ts",
    "test:pubsub": "ts-node scripts/test-pubsub.ts",
    "setup:pubsub": "ts-node scripts/setup-pubsub.ts",
    "dev:3020": "npm run backup-env && lsof -ti :3020 | xargs kill -9 2>/dev/null || true && next dev -p 3020",
    "test:langchain": "NODE_PATH=. ts-node scripts/test-langchain.ts"
  },
